<%def name="gen_row(fields)">
    <table:table-row-group>
    % for field in fields:
        <table:table-row table:style-name="ro3">
        <table:table-cell office:value-type="string" table:style-name="Result">
        <text:p>${field['name']}</text:p>
        </table:table-cell>
        <table:table-cell office:value-type="string" table:style-name="Result">
        <text:p>${field['string']}</text:p>
        </table:table-cell>
        <table:table-cell office:value-type="string" table:style-name="Result">
        <text:p>${field['help']|h}</text:p>
        </table:table-cell>
        <table:table-cell office:value-type="string" table:style-name="Result">
        <text:p>${field['type']}</text:p>
        </table:table-cell>
        <table:table-cell office:value-type="string" table:style-name="Result">
        % for attr in field['attributes']:
            % for line in attr.split("\n"):
                <text:p>${line}</text:p>
            % endfor
        % endfor
        </table:table-cell>
        <table:table-cell office:value-type="string" table:style-name="Result2">
        <text:p>${field['invisible']}</text:p>
        </table:table-cell>
        <table:table-cell office:value-type="string" table:style-name="Result2">
        <text:p>${field['readonly']}</text:p>
        </table:table-cell>
        <table:table-cell office:value-type="string" table:style-name="Result2">
        <text:p>${field['required']}</text:p>
        </table:table-cell>
        </table:table-row>
        % if field['tree']:
            <table:table-row>
            <table:table-cell office:value-type="string" table:style-name="Result3" table:number-columns-spanned="8">
            <text:p>Tree view</text:p>
            </table:table-cell>
            <table:covered-table-cell table:style-name="Default" table:number-columns-repeated="7"/>
            </table:table-row>
            ${gen_row(field['tree'])}
        % endif
        % if field['form']:
            <table:table-row>
            <table:table-cell office:value-type="string" table:style-name="Result3" table:number-columns-spanned="8">
            <text:p>From view</text:p>
            </table:table-cell>
            <table:covered-table-cell table:style-name="Default" table:number-columns-repeated="7"/>
            </table:table-row>
            ${gen_row(field['form'])}
        % endif
    % endfor
    </table:table-row-group>
</%def>

<table:table-row table:style-name="ro1">
% for header, comment in headers:
<table:table-cell table:style-name="ce1" office:value-type="string">
% if comment:
    <office:annotation draw:style-name="gr2" draw:text-style-name="P1" svg:width="2.899cm" svg:height="1.781cm" svg:x="28.678cm" svg:y="0cm" draw:caption-point-x="-0.61cm" draw:caption-point-y="0.01cm">
    <dc:date>2012-11-01T00:00:00</dc:date>
    <text:p text:style-name="P1"> 
    <text:span text:style-name="T1">${comment['title']}</text:span>
    </text:p>
    % for line in comment['help'].split("\n"):
    <text:p text:style-name="P1">${line}</text:p>
    % endfor
    <text:p text:style-name="P1"/>
    </office:annotation>
% endif
<text:p>${header}</text:p>
</table:table-cell>
% endfor
