        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="Generate instances" enabled="true">
          <stringProp name="BeanShellSampler.query">
String[] operationCenterNames = { &quot;OCA&quot;, &quot;OCB&quot;, &quot;OCP&quot;, &quot;OCG&quot;, &quot;OCBA&quot; };
int[] coordosPerOperationCenter = { 25, 25, 25, 15, 15 };
int[] projectsPerMission = { 3, 3, 3, 2, 1 };
int idProjectTypeOC = 1;
int idProjectTypeMission = 2;

StringBuilder operationCentersBuf = new StringBuilder();
StringBuilder coordinationsBuf = new StringBuilder();
StringBuilder projectsBuf = new StringBuilder();
StringBuilder groupsBuf = new StringBuilder();

log.info(&quot;Generating instances...&quot;);

for (int i = 0; i &lt; operationCenterNames.length; i++) {
	String operationCenterName = operationCenterNames[i];

                      groupsBuf.append(operationCenterName).append(&quot;,&quot;).append(idProjectTypeOC).append(&quot;\n&quot;);

	operationCentersBuf.append(UUID.randomUUID()).append(&quot;,&quot;);
	operationCentersBuf.append(operationCenterName).append(&quot;,&quot;);
	operationCentersBuf.append(&quot;gregory.bellier@geneva.msf.org&quot;).append(&quot;,&quot;);
	operationCentersBuf.append(&quot;&quot;).append(&quot;,&quot;);
	operationCentersBuf.append(&quot;&lt;value&gt;&lt;string&gt;&quot;);
	operationCentersBuf.append(operationCenterName);
	operationCentersBuf.append(&quot;&lt;/value&gt;&lt;/string&gt;&quot;).append(&quot;\n&quot;);

	for (int j = 1; j &lt;= coordosPerOperationCenter[i]; j++) {
		String coordoName = operationCenterName + &quot;_Coordo&quot; +j;

		groupsBuf.append(coordoName).append(&quot;,&quot;).append(idProjectTypeMission).append(&quot;\n&quot;);

		coordinationsBuf.append(UUID.randomUUID()).append(&quot;,&quot;);
		coordinationsBuf.append(coordoName).append(&quot;,&quot;);
		coordinationsBuf.append(&quot;gregory.bellier@geneva.msf.org&quot;).append(&quot;,&quot;);
		coordinationsBuf.append(operationCenterName).append(&quot;,&quot;);
		coordinationsBuf.append(&quot;&lt;value&gt;&lt;string&gt;&quot;);
		coordinationsBuf.append(operationCenterName);
		coordinationsBuf.append(&quot;&lt;/value&gt;&lt;/string&gt;&quot;);
		coordinationsBuf.append(&quot;&lt;value&gt;&lt;string&gt;&quot;);
		coordinationsBuf.append(coordoName);
		coordinationsBuf.append(&quot;&lt;/value&gt;&lt;/string&gt;&quot;).append(&quot;\n&quot;);

		for (int k = 1; k &lt;= projectsPerMission[i]; k++) {
			String projectName = coordoName + &quot;_Project&quot; + k;

			projectsBuf.append(UUID.randomUUID()).append(&quot;,&quot;);
			projectsBuf.append(projectName).append(&quot;,&quot;);
			projectsBuf.append(&quot;gregory.bellier@geneva.msf.org&quot;).append(&quot;,&quot;);
			projectsBuf.append(coordoName).append(&quot;,&quot;);
			projectsBuf.append(&quot;&lt;value&gt;&lt;string&gt;&quot;);
			projectsBuf.append(operationCenterName);
			projectsBuf.append(&quot;&lt;/value&gt;&lt;/string&gt;&quot;);
			projectsBuf.append(&quot;&lt;value&gt;&lt;string&gt;&quot;);
			projectsBuf.append(coordoName);
			projectsBuf.append(&quot;&lt;/value&gt;&lt;/string&gt;&quot;).append(&quot;\n&quot;);
		}

	}
}

log.info(&quot;done.&quot;);
log.info(&quot;Saving instances to files...&quot;);

try {
	FileOutputStream fos = null;

                      fos = new FileOutputStream(vars.get(&quot;groupsPool&quot;));
                      fos.write(groupsBuf.toString().getBytes());
                      fos.close();

	fos = new FileOutputStream(vars.get(&quot;operationCentersPool&quot;));
	fos.write(operationCentersBuf.toString().getBytes());
	fos.close();

	fos = new FileOutputStream(vars.get(&quot;missionsPool&quot;));
	fos.write(coordinationsBuf.toString().getBytes());
	fos.write(projectsBuf.toString().getBytes());
	fos.close();

	fos = new FileOutputStream(vars.get(&quot;coordinationsPool&quot;));
	fos.write(coordinationsBuf.toString().getBytes());
	fos.close();

	fos = new FileOutputStream(vars.get(&quot;projectsPool&quot;));
	fos.write(projectsBuf.toString().getBytes());
	fos.close();
} catch (Exception ex) {
	log.error(ex.getMessage());
	System.err.println(ex.getMessage());
}

log.info(&quot;done.&quot;);

</stringProp>
