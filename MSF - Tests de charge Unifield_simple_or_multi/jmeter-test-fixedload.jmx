                  <stringProp name="script">
import javax.xml.parsers.*;
import javax.xml.xpath.*;
import org.xml.sax.*;
import org.w3c.dom.*;
import org.apache.jmeter.samplers.*;

String xpathExpr = &quot;//methodResponse/params/param/value/array/data/value[2]/array/data/value/struct/member[name=&apos;id&apos;]/value/string&quot;;

StringBuilder sb = new StringBuilder();

try {
	DocumentBuilderFactory factory = DocumentBuilderFactory.newInstance();
	factory.setNamespaceAware(true);
	DocumentBuilder builder = factory.newDocumentBuilder();
	SampleResult result = ctx.getPreviousResult();
	InputSource is = new InputSource(new StringReader(result.getResponseDataAsString()));
	Document document = builder.parse(is);

	XPath xpath = XPathFactory.newInstance().newXPath();
	XPathExpression expr = xpath.compile(xpathExpr);

	NodeList nodes = (NodeList) expr.evaluate(document, XPathConstants.NODESET);
	for (int i = 0; i &lt; nodes.getLength(); i++) {
		Node node = nodes.item(i);
		sb.append(&quot;&lt;value&gt;&lt;string&gt;&quot;).append(node.getTextContent()).append(&quot;&lt;/value&gt;&lt;/string&gt;&quot;);
	}
} catch (Exception ex) {
	log.error(ex.getMessage());
	ex.printStackTrace();
}

vars.put(&quot;messageIds&quot;, sb.toString());
</stringProp>
