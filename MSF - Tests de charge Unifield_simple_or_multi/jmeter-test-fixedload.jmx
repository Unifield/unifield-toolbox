            <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="Generate messages" enabled="true">
              <stringProp name="BeanShellSampler.query">String dirName = (String) vars.get(&quot;dir&quot;);
if (null == dirName) {
	return;
}

File dir = new File(dirName + &quot;messages&quot;);
if (!dir.exists() || !dir.isDirectory()) {
	return;
}

String[] files = dir.list();

BufferedReader reader = null;
StringBuffer buffer = null;
String line = null;
String pkg = null;
for (int i = 0; i &lt; files.length; i++) {
	try {
		reader = new BufferedReader(new FileReader(dir.getAbsolutePath() + File.separatorChar + files[i]));
		buffer = new StringBuffer();
		while ((line = reader.readLine()) != null) {
			buffer.append(line).append(&quot;\n&quot;);
		}

		pkg = buffer.toString()
			.replaceAll(&quot;#uuid#&quot;, vars.get(&quot;uuid&quot;))
			.replaceAll(&quot;#parent_instance#&quot;, vars.get(&quot;parent_instance&quot;))
			.replaceAll(&quot;#session_id#&quot;, vars.get(&quot;session_id&quot;));

		vars.put(&quot;message_&quot; + (i + 1), pkg);
	} catch (IOException ioe) {
		log.error(ioe.getMessage());
	} finally {
		if (reader != null) {
			reader.close();
		}
	}
}
</stringProp>
